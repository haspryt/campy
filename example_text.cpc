DECLARE ScoreOne : INTEGER
DECLARE ScoreTwo : INTEGER
DECLARE HighestScore : INTEGER

ScoreOne <- INPUT
ScoreTwo <- INPUT
HighestScore <- 54000

// Checks scores
REPEAT  //test
    OUTPUT ScoreOne
    IF ScoreOne > HighestScore THEN
        OUTPUT ScoreOne, " is the new high score!", ScoreTwo
    ENDIF
//ELSE
                            //OUTPUT "PlayerOne lost!"
UNTIL (ScoreOne + 1) > ScoreTwo